<?xml version="1.0" ?>
<robot name="gripper_beam_1C_90deg_3mm">

    <!-- Import Rviz colors -->
    <material name="Grey">
        <color rgba="0.2 0.2 0.2 1."/>
    </material>
    <material name="Orange">
        <color rgba="1.0 0.423529411765 0.0392156862745 0.5"/>
    </material>
    <material name="Blue">
        <color rgba="0.5 0.7 1.0 0.5"/>
    </material>
    <material name="beam_color">
        <color rgba="0.72 0.60 0.30 1.0"/>
    </material>


    <!-- Joint from Base to FT-Sensor Below -->
    <joint name="joint_ft" type="fixed">
        <parent link="base"/>
        <child link="ft_sensor"/>
        <origin rpy="0 0 0 " xyz="0.0 0.0 0.0"/>
        <dynamics damping="0.5"/>
    </joint>

    <!-- Joint from FT-Sensor to Gripper-Beam Below -->
    <joint name="joint_gripper" type="fixed">
        <parent link="ft_sensor"/>
        <child link="gripper_beam_1C_90deg_3mm"/>
        <origin rpy="0 0 0 " xyz="0.0 0.0 0.0"/>
        <dynamics damping="0.5"/>
    </joint>

    <!-- Joint from Gripper-Beam to Virtual Member Below -->
    <joint name="joint_member" type="fixed">
        <parent link="gripper_beam_1C_90deg_3mm"/>
        <child link="member"/>
        <!-- Offset 40 mm (beam half-depth) + 155 mm (beam offset from world origin)-->
        <origin rpy="0 0 0 " xyz="0.0 0.0 -0.195"/>
        <dynamics damping="0.5"/>
    </joint>


    <!-- Virtual Base Link -->
    <link name="base">
      <inertial>
          <origin rpy="0 0 0" xyz="0 0 0"/>
          <mass value="0.001"/>
          <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
      </inertial>
    </link>

    <!-- Virtual Member Link to Computer Distance to Target -->
    <link name="member">
      <inertial>
          <origin rpy="0 0 0" xyz="0 0 0"/>
          <mass value="0.01"/>
          <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
      </inertial>
    </link>

    <!-- FT-Sensor Link -->
    <link name="ft_sensor">
    <inertial>
          <mass value="1.964"/>
          <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
    <visual>
        <material name="Blue"/>
        <geometry>
          <mesh filename="sensor/meshes/visual/ft_sensor.stl"/>
        </geometry>
    </visual>
    <collision>
        <geometry>
          <mesh filename="sensor/meshes/collision/ft_sensor.stl"/>
        </geometry>
    </collision>
    </link>

    <!-- EF Link -->
    <link name="gripper_beam_1C_90deg_3mm">
        <inertial>
            <!--center of mass origin and mass should was updated from ABB's calibration routine 20.02.2019-->
            <origin rpy="0 0 0" xyz="0.0 0.0 0.09963736"/>
            <mass value="8.56"/>
            <!--origin rpy="0 0 0" xyz="-8.173542509136381e-19 4.919694157880184e-18 -0.12804086440536944"/-->
            <!--mass value="4.310275841129159"/-->
