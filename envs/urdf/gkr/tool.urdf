            <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="gripper_beam_1C_90deg_3mm/meshes/visual/gripper_beam_1C_90deg_3mm.stl"/>
